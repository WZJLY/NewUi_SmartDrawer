<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserDialog">

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="634dp"
        android:layout_height="690dp"
        android:background="@drawable/fc_12"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="634dp"
        android:layout_height="104dp"
        android:src="@drawable/background_drawer_12"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:layout_constraintTop_toTopOf="@+id/imageView26" />

    <TextView
        android:id="@+id/tv_Duser_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="新增人员"
        android:textColor="#fff"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView14"
        app:layout_constraintEnd_toEndOf="@+id/imageView14"
        app:layout_constraintStart_toStartOf="@+id/imageView14"
        app:layout_constraintTop_toTopOf="@+id/imageView14" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="8dp"
        android:text="学/工号"
        android:textColor="#808080"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_name2"
        app:layout_constraintEnd_toStartOf="@+id/et_Duser_name2"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_name2" />

    <EditText
        android:id="@+id/et_Duser_name3"
        android:layout_width="280dp"
        android:layout_height="41dp"
        android:layout_marginEnd="148dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_edit_style"
        android:inputType="number|phone"
        android:textColor="#676767"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/et_Duser_name2" />

    <EditText
        android:id="@+id/et_Duser_name2"
        android:layout_width="280dp"
        android:layout_height="41dp"
        android:layout_marginEnd="148dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_edit_style"
        android:inputType="phone"
        android:textColor="#676767"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/et_Duser_name" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="8dp"
        android:text="级别"
        android:textColor="#808080"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/rg_Duser_level"
        app:layout_constraintEnd_toStartOf="@+id/rg_Duser_level"
        app:layout_constraintTop_toTopOf="@+id/rg_Duser_level" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:text="手机号"
        android:textColor="#808080"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_name3"
        app:layout_constraintEnd_toStartOf="@+id/et_Duser_name3"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_name3" />

    <RadioGroup
        android:id="@+id/rg_Duser_level"
        android:layout_width="280dp"
        android:layout_height="41dp"
        android:layout_marginEnd="148dp"
        android:layout_marginTop="10dp"
        android:checkedButton="@+id/rb_Duser_nomal"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/et_Duser_password2">

        <RadioButton
            android:id="@+id/rb_Duser_admin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="管理员"
            android:textColor="@color/btn_32_textstyle"
            android:textSize="20sp" />

        <RadioButton
            android:id="@+id/rb_Duser_nomal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="普通用户"
            android:textColor="@color/btn_32_textstyle"
            android:textSize="20sp" />
    </RadioGroup>

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="(必填）"
        android:textColor="#E86344"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_name"
        app:layout_constraintStart_toEndOf="@+id/et_Duser_name"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_name" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="(必填）"
        android:textColor="#E86344"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/rg_Duser_level"
        app:layout_constraintStart_toEndOf="@+id/rg_Duser_level"
        app:layout_constraintTop_toTopOf="@+id/rg_Duser_level" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="(必填）"
        android:textColor="#E86344"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_password2"
        app:layout_constraintStart_toEndOf="@+id/et_Duser_password2"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_password2" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="(必填）"
        android:textColor="#E86344"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_password"
        app:layout_constraintStart_toEndOf="@+id/et_Duser_password"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_password" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="(必填）"
        android:textColor="#E86344"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Dimport_name"
        app:layout_constraintStart_toEndOf="@+id/et_Duser_account"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_account" />

    <EditText
        android:id="@+id/et_Duser_account"
        android:layout_width="280dp"
        android:layout_height="41dp"
        android:layout_marginEnd="148dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/bg_edit_style"
        android:textColor="#676767"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <EditText
        android:id="@+id/et_Duser_password"
        android:layout_width="280dp"
        android:layout_height="41dp"
        android:layout_marginEnd="148dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_edit_style"
        android:inputType="textPassword"
        android:textColor="#676767"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/et_Duser_account" />

    <EditText
        android:id="@+id/et_Duser_password2"
        android:layout_width="280dp"
        android:layout_height="41dp"
        android:layout_marginEnd="148dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_edit_style"
        android:inputType="textPassword"
        android:textColor="#676767"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/et_Duser_password" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:text="密码"
        android:textColor="#808080"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_password"
        app:layout_constraintEnd_toStartOf="@+id/et_Duser_password"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_password" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:text="确认密码"
        android:textColor="#808080"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_password2"
        app:layout_constraintEnd_toStartOf="@+id/et_Duser_password2"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_password2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:text="姓名"
        android:textColor="#808080"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_name"
        app:layout_constraintEnd_toStartOf="@+id/et_Duser_name"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_name" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="8dp"
        android:text="账号"
        android:textColor="#808080"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/et_Duser_account"
        app:layout_constraintEnd_toStartOf="@+id/et_Duser_account"
        app:layout_constraintTop_toTopOf="@+id/et_Duser_account" />

    <EditText
        android:id="@+id/et_Duser_name"
        android:layout_width="280dp"
        android:layout_height="41dp"
        android:layout_marginEnd="148dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/bg_edit_style"
        android:textColor="#676767"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/rg_Duser_level" />

    <Button
        android:id="@+id/btn_Duser_OK"
        android:layout_width="320dp"
        android:layout_height="54dp"
        android:layout_marginTop="37dp"
        android:background="@drawable/btn_34_style"
        android:text="确认添加"
        android:textColor="#fff"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/et_Duser_name3" />

    <Button
        android:id="@+id/button4"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@android:color/transparent"
        android:text="取消"
        android:textColor="#328BD8"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:layout_constraintTop_toBottomOf="@+id/btn_Duser_OK" />
</android.support.constraint.ConstraintLayout>