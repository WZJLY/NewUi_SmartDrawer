<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    tools:context=".DrawerFragment2">


    <ImageButton
        android:id="@+id/ib_Fdrawer2_op"
        android:layout_width="752dp"
        android:layout_height="80dp"
        android:layout_marginTop="34dp"
        android:src="@drawable/background_drawer_style_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_Fdrawer2_drawer"
        android:layout_width="96dp"
        android:layout_height="4dp"
        android:layout_marginTop="29dp"
        android:background="@android:color/transparent"
        android:src="@drawable/image_6_fff"
        app:layout_constraintEnd_toEndOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintStart_toStartOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintTop_toTopOf="@+id/ib_Fdrawer2_op" />

    <TextView
        android:id="@+id/tv_Fdrawer2_drawerNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="抽屉1"
        android:textColor="#D5EBFF"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintTop_toTopOf="@+id/ib_Fdrawer2_op" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="2dp"
        android:layout_height="48dp"
        android:layout_marginEnd="100dp"
        android:src="@drawable/image_drawer"
        app:layout_constraintBottom_toBottomOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintEnd_toEndOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintTop_toTopOf="@+id/ib_Fdrawer2_op" />

    <ImageButton
        android:id="@+id/ib_Fdrawer2_lock"
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:background="@android:color/transparent"
        android:src="@drawable/lock2"
        app:layout_constraintBottom_toBottomOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintEnd_toEndOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintStart_toEndOf="@+id/imageView27"
        app:layout_constraintTop_toTopOf="@+id/ib_Fdrawer2_op" />

    <FrameLayout
        android:id="@+id/fl_Fdrawer2_table"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintStart_toStartOf="@+id/ib_Fdrawer2_op"
        app:layout_constraintTop_toBottomOf="@+id/ib_Fdrawer2_op">

    </FrameLayout>

</android.support.constraint.ConstraintLayout>